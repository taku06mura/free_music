.new
  NEW_SOUND
  = form_for :music, url:musics_path,:multipart => true do |f|
    = f.text_field :name,placeholder: 'name'
    %br/
    = f.text_field :BPM,placeholder: 'BPM'
    %br/
    = f.label :mp3 do
      = f.file_field :mp3 ,accept:'audio/mp3'
    %br/
    = f.text_area :youtube,placeholder: 'URL'
    %br/
    = f.submit
    -# = f.text_area :mp3,placeholder: 'MP3'
