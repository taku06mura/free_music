.edit
  EDIT_SOUND
  = form_for :music, url: music_path, method: :patch do |f|
    = f.text_field :name,placeholder: 'name'
    %br/
    = f.text_field :BPM,placeholder: 'BPM'
    %br/
    = f.text_area :mp3,placeholder: 'MP3'
    %br/
    = f.text_area :youtube,placeholder: 'URL'
    %br/
    = f.submit